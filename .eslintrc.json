{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [0],
      "max-len": [0],
      "no-shadow": [0],
      "jsx-a11y/href-no-hash": [0],
      "react/prop-types": [0],
      "react/jsx-boolean-value": [0],
      "no-unused-vars": [1],
      "jsx-a11y/label-has-for": [1]
    },
    "globals": {
      "document": true,
      "fetch": true
    },
    "env": {
      "jest": true
    }
}
